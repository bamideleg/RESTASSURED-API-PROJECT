#driver=webdriver.firefox(executable_path="C:\browserDrivers\geckodriverexe.exe")

driver=webdriver.chrome(executable_path="\C:\BrowserDrivers\chromedriver.exe")
driver.get("https://www.jobserve.com/")
print(driver.title)
print(driver.current_url)
driver(driver.page.source)

driver.close()
